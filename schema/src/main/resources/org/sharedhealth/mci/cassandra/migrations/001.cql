CREATE TABLE IF NOT EXISTS patient (
  health_id varchar,
  national_id varchar,
  uid varchar,
  bin_brn varchar,

  given_name varchar,
  sur_name varchar,
  full_name_bangla varchar,

  fathers_nid varchar,
  fathers_uid varchar,
  fathers_brn varchar,
  fathers_given_name varchar,
  fathers_sur_name varchar,
  fathers_name_bangla varchar,
  mothers_nid varchar,
  mothers_uid varchar,
  mothers_brn varchar,
  mothers_given_name varchar,
  mothers_sur_name varchar,
  mothers_name_bangla varchar,

  household_code varchar,
  address_line varchar,
  division_id varchar,
  district_id varchar,
  upazila_id varchar,
  city_corporation_id varchar,
  union_or_urban_ward_id varchar,
  rural_ward_id varchar,
  area_mouja varchar,
  street varchar,
  village varchar,
  post_code varchar,
  post_office varchar,
  holding_number varchar,
  country_code varchar,

  permanent_address_line varchar,
  permanent_division_id varchar,
  permanent_district_id varchar,
  permanent_upazila_id varchar,
  permanent_city_corporation_id varchar,
  permanent_union_or_urban_ward_id varchar,
  permanent_rural_ward_id  varchar,
  permanent_area_mouja varchar,
  permanent_country_code varchar,
  permanent_holding_number varchar,
  permanent_post_code varchar,
  permanent_post_office varchar,
  permanent_street varchar,
  permanent_village varchar,

  marriage_id varchar,
  marital_relations varchar,
  marital_status varchar,

  blood_group varchar,
  date_of_birth timestamp,
  disability varchar,
  ethnicity varchar,
  edu_level varchar,
  gender varchar,
  nationality varchar,
  occupation varchar,
  place_of_birth varchar,
  relations varchar,
  religion varchar,
  primary_contact varchar,

  phone_no varchar,
  phone_number_area_code varchar,
  phone_number_country_code varchar,
  phone_number_extension varchar,

  primary_contact_no varchar,
  primary_contact_number_area_code varchar,
  primary_contact_number_country_code varchar,
  primary_contact_number_extension varchar,

  pending_approvals varchar,

  confidential boolean,
  status varchar,
  date_of_death timestamp,
  created_at timeuuid,
  created_by varchar,
  updated_at timeuuid,
  updated_by varchar,

  PRIMARY KEY (health_id)
